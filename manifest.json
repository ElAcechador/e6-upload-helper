{
	"manifest_version": 2,
	"name": "e6-upload-helper",
	"version": "0.0.1",

	"description": "",

	"permissions": [
		"menus"
	],

	"background": {
		"scripts": ["background.js"]
	},

	"content_scripts": [
		{
			"matches": ["*://*.twitter.com/*"],
			"js": ["search-scripts/twitter.js"]
		},{
			"matches": ["*://*.tumblr.com/*"],
			"js": ["search-scripts/tumblr.js"]
		}
	],
	
	"icons": {
		"16": "icons/favicon-16x16.png",
		"32": "icons/favicon-32x32.png",
		"512": "icons/android-chrome-512x512.png"
	}
}